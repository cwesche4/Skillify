    "ts-node": "^10.9.2",
    "tsx": "^4.21.0",
    "turbo": "^2.6.2",
    "typescript": "^5.6.3",
    "vite": "^5.4.21",
    "vitest": "^1.6.1"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{json,md,css,scss}": [
      "prettier --write"
    ]
  }
}